<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE siop_document [
<!ELEMENT siop_document (levels , default_scenario , extra_scenario?)>
<!ELEMENT levels (level)+>
<!ELEMENT level EMPTY>
<!ATTLIST level
          id CDATA #REQUIRED
          temp CDATA #REQUIRED>
<!ELEMENT default_scenario (limiter)+>
<!ELEMENT limiter (sts_item* , table_item*)>
<!ATTLIST limiter
          name CDATA #REQUIRED
          type (STS|number|bool) #REQUIRED>
<!ELEMENT sts_item EMPTY>
<!ATTLIST sts_item
          pre CDATA #IMPLIED
          perc CDATA #IMPLIED
          trigger CDATA #REQUIRED
          release CDATA #REQUIRED
          min CDATA #IMPLIED
          max CDATA #IMPLIED
          duration CDATA #IMPLIED>
<!ELEMENT table_item EMPTY>
<!ATTLIST table_item
          temp CDATA #REQUIRED
          value CDATA #REQUIRED
          low_temp CDATA #IMPLIED>
<!ELEMENT extra_scenario (scenario)+>
<!ELEMENT scenario (trigger_statuses? , trigger_packages? , limiter*)>
<!ATTLIST scenario name CDATA #REQUIRED>
<!ATTLIST scenario priority CDATA #IMPLIED>
<!ELEMENT trigger_statuses (status)+>
<!ELEMENT status EMPTY>
<!ATTLIST status name CDATA #REQUIRED>
<!ELEMENT trigger_packages (package)+>
<!ELEMENT package EMPTY>
<!ATTLIST package name CDATA #REQUIRED>
] >

<siop_document>

    <model_settings>
        <setting name="OverheatComplexControl">
            <MW pst="510" bat="450" />
            <LTB pst="510" bat="450" />
        </setting>
        <setting name="TemperatureGap">
            <set model_gap="80" />
        </setting>
    </model_settings>

    <levels>
        <level id="-3" temp="350" />
        <level id="-2" temp="370" />
        <level id="-1" temp="390" />
        <level id="0" temp="410" />
        <level id="1" temp="430" />
        <level id="2" temp="450" />
        <level id="3" temp="470" />
        <level id="4" temp="490" />
        <level id="5" temp="510" />
        <level id="6" temp="530" />
    </levels>

    <default_scenario>
        <limiter name="CPUFreqMax" type="number">
            <table_item temp="430" value="-1" />
            <table_item temp="450" value="1689600" />
            <table_item temp="470" value="1401600" />
            <table_item temp="490" value="1401600" />
            <table_item temp="510" value="1036800" />
        </limiter>
        <limiter name="GPUFreqMax" type="number">
            <table_item temp="430" value="-1" />
            <table_item temp="450" value="560000000" />
            <table_item temp="470" value="510000000" />
            <table_item temp="490" value="510000000" />
            <table_item temp="510" value="400000000" />
            <table_item temp="530" value="320000000" />
        </limiter>
        <limiter name="PST" type="number">
            <table_item temp="410" value="1" />
        </limiter>
        <limiter name="Charging" type="number" formula="PST*BAT">
            <table_item pst="0" bat="300" value="-1" />
            <table_item pst="300" bat="300" value="70" />
            <table_item pst="450" bat="300" value="70" />
            <table_item pst="470" bat="300" value="10" />
            <table_item pst="510" bat="300" value="10" />
            <table_item pst="530" bat="300" value="0" />
        </limiter>
        <limiter name="ChargingEvent" type="number">
            <table_item temp="410" value="0" />
        </limiter>
        <limiter name="LCDBrightness" type="number">
            <table_item temp="470" value="-1" />
            <table_item temp="490" value="193" />
            <table_item temp="510" value="130" />
        </limiter>
        <limiter name="AppInstallStandby" type="number">
            <table_item temp="370" value="-1" />
            <table_item temp="390" value="3000" />
            <table_item temp="410" value="5000" />
        </limiter>
    </default_scenario>

    <extra_scenario>

